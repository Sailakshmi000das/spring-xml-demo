<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                         http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="actor" class="com.stackroute.domain.Actor">
        <property name="name" value="sai"/>
        <property name="gender" value="female"/>
        <property name="age" value="22"/>
    </bean>


    <bean id="movie" class="com.stackroute.domain.Movie" autowire="byName">
       <property name="actor" value="actor"/>
    </bean>

    <bean id="actor" class="com.stackroute.domain.Actor">
        <property name="name" value="vinay"/>
        <property name="gender" value="male"/>
        <property name="age" value="27"/>
    </bean>

    <bean id="movie1" class="com.stackroute.domain.Movie" autowire="byType">
        <property name="movieName" value="Hello"/>
        <property name="movieId" value="123"/>
    </bean>
</beans>

